import java.util.Arrays;
 
public class Main
{
    
    public static void main(String[] args)
    {
        int r = 3, c = 3;
        int mat[][]= { {1,3,5}, {2,6,9}, {3,6,9} };
        
        int[] arr;
         
     
        arr = new int[9];
        int x=0;
        
        for(int i=0; i<3; i++){
            for(int j=0; j<3; j++){
                arr[x++] = mat[i][j];
            }
        }
    
        for(int i=0; i<9; i++){
            for(int j=i+1; j<9; j++){ 
                if(arr[i]>arr[j]){
                    int temp = arr[i];
                    arr[i] = arr[j];
                    arr[j] = temp;
                }
            }
        }
    
        System.out.println("Median of the given matrix is : "+ arr[4]);
      
    }
}
